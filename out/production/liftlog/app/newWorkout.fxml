<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.Spinner?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<BorderPane prefHeight="710.0" prefWidth="880.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="app.NewWorkoutController">
   <left>
      <VBox prefHeight="710.0" prefWidth="230.0" spacing="20.0" BorderPane.alignment="CENTER">
         <children>
            <VBox prefHeight="42.0" prefWidth="230.0">
               <children>
                  <Label text="Date">
                     <font>
                        <Font name="System Bold" size="15.0" />
                     </font>
                  </Label>
                  <Label fx:id="dateLabel" prefHeight="17.0" prefWidth="90.0" text="02.15.2018" textFill="#ee1717">
                     <font>
                        <Font size="15.0" />
                     </font>
                  </Label>
                  <DatePicker fx:id="datePicker" prefHeight="29.0" prefWidth="171.0" />
               </children>
               <VBox.margin>
                  <Insets top="30.0" />
               </VBox.margin>
            </VBox>
            <VBox>
               <children>
                  <Label text="Time">
                     <font>
                        <Font name="System Bold" size="15.0" />
                     </font>
                  </Label>
                  <Label fx:id="timeLabel" prefHeight="8.0" prefWidth="90.0" text="20:00:00" textFill="#ee1717">
                     <font>
                        <Font size="15.0" />
                     </font>
                  </Label>
                  <TextField fx:id="timeTextField" maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="28.0" prefWidth="80.0" promptText="20:00:00" />
               </children>
            </VBox>
            <VBox>
               <children>
                  <Label text="Duration (minutes)">
                     <font>
                        <Font name="System Bold" size="15.0" />
                     </font>
                  </Label>
                  <Label fx:id="durationLabel" prefHeight="17.0" prefWidth="90.0" text="90" textFill="#ee1717">
                     <font>
                        <Font size="15.0" />
                     </font>
                  </Label>
                  <Spinner fx:id="durationSpinner" />
               </children>
            </VBox>
            <VBox>
               <children>
                  <Label text="Shape">
                     <font>
                        <Font name="System Bold" size="15.0" />
                     </font>
                  </Label>
                  <Label fx:id="shapeLabel" prefHeight="17.0" prefWidth="90.0" text="10" textFill="#ee1717">
                     <font>
                        <Font size="15.0" />
                     </font>
                  </Label>
                  <Spinner fx:id="shapeSpinner" />
               </children>
            </VBox>
            <VBox>
               <children>
                  <Label text="Performance">
                     <font>
                        <Font name="System Bold" size="15.0" />
                     </font>
                  </Label>
                  <Label fx:id="performanceLabel" prefHeight="17.0" prefWidth="90.0" text="10" textFill="#ee1717">
                     <font>
                        <Font size="15.0" />
                     </font>
                  </Label>
                  <Spinner fx:id="performanceSpinner" />
               </children>
            </VBox>
            <VBox>
               <children>
                  <Label text="Note">
                     <font>
                        <Font name="System Bold" size="15.0" />
                     </font>
                  </Label>
                  <Label fx:id="noteLabel" maxWidth="-Infinity" prefHeight="19.0" prefWidth="180.0" text="blablablabla" textFill="#ee1717" wrapText="true">
                     <font>
                        <Font size="15.0" />
                     </font>
                  </Label>
                  <TextArea fx:id="noteTextArea" prefHeight="175.0" prefWidth="230.0" wrapText="true" />
               </children>
            </VBox>
            <HBox alignment="BOTTOM_RIGHT" prefHeight="35.0" prefWidth="230.0" spacing="10.0">
               <children>
                  <Button fx:id="discardButton" mnemonicParsing="false" onAction="#discardButtonPressed" text="Discard" />
                  <Button fx:id="createButton" mnemonicParsing="false" onAction="#createButtonPressed" text="Create" />
               </children>
               <VBox.margin>
                  <Insets bottom="10.0" />
               </VBox.margin>
            </HBox>
         </children>
         <BorderPane.margin>
            <Insets left="20.0" />
         </BorderPane.margin>
      </VBox>
   </left>
   <right>
      <VBox fx:id="exerciseSelectionBox" prefHeight="710.0" prefWidth="200.0" spacing="10.0" BorderPane.alignment="CENTER">
         <children>
            <Label text="Equipment Exercises">
               <font>
                  <Font size="17.0" />
               </font>
            </Label>
            <ComboBox prefWidth="150.0" />
            <ListView fx:id="equipmentExerciseList" editable="true" onEditStart="#equipmentExerciseSelected" prefHeight="284.0" prefWidth="200.0" />
            <Label text="Regular Exercises">
               <font>
                  <Font size="17.0" />
               </font>
            </Label>
            <ComboBox prefWidth="150.0" />
            <ListView fx:id="regularExerciseList" prefHeight="301.0" prefWidth="189.0" />
         </children>
         <BorderPane.margin>
            <Insets bottom="20.0" right="20.0" />
         </BorderPane.margin>
      </VBox>
   </right>
   <center>
      <HBox prefHeight="100.0" prefWidth="200.0" BorderPane.alignment="CENTER">
         <children>
            <VBox fx:id="selectedExercisesBox" prefHeight="710.0" prefWidth="178.0">
               <children>
                  <Label fx:id="selectedEquipmentExerciseLabel1" prefHeight="16.0" prefWidth="125.0" text="Exercises">
                     <font>
                        <Font name="System Bold" size="20.0" />
                     </font>
                     <VBox.margin>
                        <Insets bottom="13.0" right="20.0" top="30.0" />
                     </VBox.margin>
                  </Label>
                  <ListView fx:id="exerciseInWorkoutList" editable="true" onEditStart="#selectedExerciseClicked" prefHeight="621.0" prefWidth="138.0">
                     <VBox.margin>
                        <Insets right="40.0" />
                     </VBox.margin>
                  </ListView>
               </children></VBox>
            <VBox>
               <HBox.margin>
                  <Insets right="20.0" />
               </HBox.margin>
               <children>
                  <VBox fx:id="addEquipmentSetsBox" prefHeight="339.0" prefWidth="213.0">
                     <children>
                        <Label fx:id="selectedEquipmentExerciseLabel" prefHeight="16.0" prefWidth="125.0" text="Deadlift">
                           <VBox.margin>
                              <Insets top="30.0" />
                           </VBox.margin>
                           <font>
                              <Font name="System Bold" size="20.0" />
                           </font>
                        </Label>
                        <HBox alignment="BOTTOM_RIGHT" prefHeight="42.0" prefWidth="252.0">
                           <children>
                              <Label prefHeight="16.0" prefWidth="197.0" text="Weight (kg)">
                                 <font>
                                    <Font size="15.0" />
                                 </font>
                              </Label>
                              <Label prefHeight="16.0" prefWidth="191.0" text="Reps">
                                 <font>
                                    <Font size="15.0" />
                                 </font>
                              </Label>
                           </children>
                           <VBox.margin>
                              <Insets bottom="5.0" />
                           </VBox.margin>
                        </HBox>
                        <HBox prefHeight="36.0" prefWidth="242.0">
                           <children>
                              <Spinner fx:id="weightSpinner" />
                              <Spinner fx:id="repsSpinner" />
                           </children>
                        </HBox>
                        <HBox alignment="TOP_RIGHT" prefHeight="53.0" prefWidth="242.0">
                           <children>
                              <Button fx:id="addEquipmentSetButton" mnemonicParsing="false" onAction="#addEquipmentSetButtonPressed" text="Add" />
                           </children>
                        </HBox>
                        <ListView fx:id="equipmentExerciseSetList" prefHeight="141.0" prefWidth="213.0" />
                     </children>
                  </VBox>
                  <VBox fx:id="addRegularSetsBox" prefHeight="361.0" prefWidth="213.0">
                     <children>
                        <Label fx:id="selectedRegularExerciseLabel" prefHeight="16.0" prefWidth="125.0" text="Deadlift">
                           <VBox.margin>
                              <Insets top="30.0" />
                           </VBox.margin>
                           <font>
                              <Font name="System Bold" size="20.0" />
                           </font>
                        </Label>
                        <HBox alignment="BOTTOM_RIGHT" prefHeight="42.0" prefWidth="252.0">
                           <children>
                              <Label prefHeight="16.0" prefWidth="197.0" text="Weight (kg)">
                                 <font>
                                    <Font size="15.0" />
                                 </font>
                              </Label>
                              <Label prefHeight="16.0" prefWidth="191.0" text="Reps">
                                 <font>
                                    <Font size="15.0" />
                                 </font>
                              </Label>
                           </children>
                           <VBox.margin>
                              <Insets bottom="5.0" />
                           </VBox.margin>
                        </HBox>
                        <HBox prefHeight="36.0" prefWidth="242.0">
                           <children>
                              <Spinner />
                              <Spinner />
                           </children>
                        </HBox>
                        <HBox alignment="TOP_RIGHT" prefHeight="53.0" prefWidth="242.0">
                           <children>
                              <Button fx:id="addRegularSetButton" mnemonicParsing="false" text="Add" />
                           </children>
                        </HBox>
                        <ListView fx:id="regularExerciseSetList" prefHeight="159.0" prefWidth="213.0" />
                     </children>
                  </VBox>
               </children>
            </VBox>
         </children>
      </HBox>
   </center>
</BorderPane>
