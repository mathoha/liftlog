            </TableView>
            <TableView fx:id="equipmentExerciseTable" editable="true" layoutX="459.0" layoutY="2.0" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="331.0">
