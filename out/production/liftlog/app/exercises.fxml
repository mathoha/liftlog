               </columns>
            </TableView>
         </children>
      </AnchorPane>
      <Button fx:id="addRegularExerciseButton" layoutX="147.0" layoutY="497.0" mnemonicParsing="false" onAction="#addRegularExerciseButtonPressed" text="Add Exercise" />
      <Button fx:id="addEquipmentExerciseButton" layoutX="591.0" layoutY="497.0" mnemonicParsing="false" onAction="#addEquipmentExerciseButtonPressed" text="Add Exercise" />
