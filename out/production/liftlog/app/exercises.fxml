<AnchorPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="app.ExercisesController">
   <children>
      <HBox prefHeight="52.0" prefWidth="800.0">
         <children>
            <Label prefHeight="52.0" prefWidth="462.0" text="Regular Exercises">
               <padding>
                  <Insets left="50.0" />
               </padding>
               <font>
                  <Font size="17.0" />
               </font>
            </Label>
            <Label prefHeight="52.0" prefWidth="258.0" text="Equipment Exercises">
               <padding>
                  <Insets left="10.0" />
               </padding>
               <font>
                  <Font size="17.0" />
               </font>
            </Label>
         </children>
      </HBox>
      <AnchorPane layoutY="51.0" prefHeight="400.0" prefWidth="800.0">
         <children>
            <TableView fx:id="regularExerciseTable" editable="true" layoutX="38.0" layoutY="3.0" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="331.0">
              <columns>
                <TableColumn fx:id="col_regularExerciseName" prefWidth="124.0" text="name" />
                  <TableColumn fx:id="col_regularExerciseDescription" maxWidth="1.7976931348623157E308" prefWidth="206.0" text="description" />
            
                                </columns>
               <columnResizePolicy>
                  <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
